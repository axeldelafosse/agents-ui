{
  "name": "@axel-delafosse/protocol",
  "version": "0.1.2",
  "private": false,
  "description": "Shared protocol adapters, message parsing, and types for agents-ui transports.",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./*": {
      "bun": "./src/*.ts",
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "agents",
    "protocol",
    "codex",
    "claude",
    "streaming"
  ],
  "scripts": {
    "build": "tsup",
    "test": "bun test"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
